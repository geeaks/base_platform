package com.ucf.customer.utils.enums;

/**
 * @Description: 短信服务渠道路由
 * @ClassName: EnumSMSRouter
 * @author gaoxiang
 * @date 2015-5-20 下午6:06:13
 */ 
public enum EnumSMSRouter {

	MANDAO("MANDAO","漫道短信服务渠道"),
	WANGXIN("WANGXIN","网信理财服务渠道");
	
	private String code;
	private String msg;

	private EnumSMSRouter(String code, String msg) {
		this.code = code;
		this.msg = msg;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	
}
