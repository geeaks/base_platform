    /**
     * 解析图片路径集合
     * @author yuzengjia
     * @date 2014-6-18 下午4:38:28
     * @return
     */
    public List<String> getImgPathList(){
    	if(imgPath==null||"".equals(imgPath))return null;
    	String[] imgArr = imgPath.split("\\|");
    	List<String> imgList = new ArrayList<String>(imgArr.length);
    	for(String path:imgArr){
    		imgList.add(path);
    	}
    	return imgList;
    }
