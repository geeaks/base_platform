<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ucf.customer">


	<resultMap id="BaseResultMap" type="ucfRealnamePhotoApply">
		<result column="ID" property="id" jdbcType="VARCHAR" />
		<result column="USER_ID" property="userId" jdbcType="VARCHAR" />
		<result column="BANK_CARD_NO" property="bankCardNo" jdbcType="VARCHAR" />
		<result column="IMG_PATH" property="imgPath" jdbcType="VARCHAR" />
		<result column="STATUS" property="status" jdbcType="VARCHAR" />
		<result column="MEMO" property="memo" jdbcType="VARCHAR" />
		<result column="GMT_CREATE" property="gmtCreate" jdbcType="TIMESTAMP" />
		<result column="GMT_MODIFY" property="gmtModify" jdbcType="TIMESTAMP" />
		<result column="GMT_VERIFY" property="gmtVerify" jdbcType="TIMESTAMP" />
		<result column="CREATEOR" property="createor" jdbcType="VARCHAR" />
		<result column="MODIFYER" property="modifyer" jdbcType="VARCHAR" />
		<result column="VERIFYER" property="verifyer" jdbcType="VARCHAR" />
		<result column="TYPE" property="type" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="Base_Column_List">
		ID, USER_ID,BANK_CARD_NO,IMG_PATH, STATUS, MEMO, GMT_CREATE,
		GMT_MODIFY, CREATEOR,TYPE,
		MODIFYER,VERIFYER,GMT_VERIFY
	</sql>

	<select id="selectByPrimaryKey" resultMap="BaseResultMap"
		parameterType="ucfRealnamePhotoApply">
		select
		<include refid="com.ucf.customer.Base_Column_List" />
		from UCF_REALNAME_PHOTO_APPLY
		where ID = #{id}
	</select>
	

	<select id="select" resultMap="BaseResultMap" parameterType="ucfRealnamePhotoApply">
		select
		<include refid="com.ucf.customer.Base_Column_List" />
		from UCF_REALNAME_PHOTO_APPLY
		where 1=1 
		<if test="null != userId">
			AND USER_ID = #{userId,jdbcType=VARCHAR}
		</if>
		<if test="null != bankCardNo">
			AND BANK_CARD_NO = #{bankCardNo,jdbcType=VARCHAR}
		</if>
		<if test="null != status">
			AND  STATUS = #{status,jdbcType=VARCHAR}
		</if>
		<if test="null != createor">
			AND CREATEOR = #{createor,jdbcType=VARCHAR}
		</if>
		<if test="null != modifyer">
			AND  MODIFYER = #{modifyer,jdbcType=VARCHAR}
		</if>
		<if test="null != type">
			AND  TYPE = #{type,jdbcType=VARCHAR}
		</if>
		 ORDER BY GMT_CREATE 
	</select>

	<insert id="insert" parameterType="ucfRealnamePhotoApply">
		insert into
		UCF_REALNAME_PHOTO_APPLY (ID, USER_ID,BANK_CARD_NO,IMG_PATH,
		STATUS, MEMO,GMT_CREATE, GMT_MODIFY, CREATEOR, MODIFYER,TYPE,VERIFYER)
		values
		(#{id,jdbcType=VARCHAR}, 
		#{userId,jdbcType=VARCHAR},
		#{bankCardNo,jdbcType=VARCHAR},
		#{imgPath,jdbcType=VARCHAR},
		#{status,jdbcType=VARCHAR},
		#{memo,jdbcType=VARCHAR},
		sysdate, sysdate,
		#{createor,jdbcType=VARCHAR},
		#{modifyer,jdbcType=VARCHAR},
		#{type,jdbcType=VARCHAR},
		#{verifyer,jdbcType=VARCHAR}
		)
	</insert>


	<update id="updateByPrimaryKeySelective" parameterType="ucfRealnamePhotoApply">
		update UCF_REALNAME_PHOTO_APPLY
		set
		USER_ID = #{userId,jdbcType=VARCHAR}
		<if test="null != status">
			,STATUS = #{status,jdbcType=VARCHAR}
		</if>
		<if test="null != memo">
			,MEMO = #{memo,jdbcType=VARCHAR}
		</if>
		,GMT_MODIFY = sysdate
		<if test="null != gmtVerify">
			,GMT_VERIFY = #{gmtVerify,jdbcType=VARCHAR}
		</if>
		<if test="null != createor">
			,CREATEOR = #{createor,jdbcType=VARCHAR}
		</if>
		<if test="null != modifyer">
			,MODIFYER = #{modifyer,jdbcType=VARCHAR}
		</if>
		<if test="null != verifyer">
			,VERIFYER = #{verifyer,jdbcType=VARCHAR}
		</if>
		<if test="null != type">
			,TYPE = #{type,jdbcType=VARCHAR}
		</if>
		where ID = #{id,jdbcType=VARCHAR}
	</update>
	
	<select id="queryUcfRealnamePhotoApplyId"  resultType="java.lang.String">
		select UCF_REALNAME_PHOTO_APPLY_SEQ.nextval from dual
	</select>
</mapper>