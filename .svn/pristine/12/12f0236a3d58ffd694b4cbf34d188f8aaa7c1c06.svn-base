<%@ page contentType="text/html;charset=UTF-8" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>先锋支付--用户login</title>
<jsp:include page="/WEB-INF/views/jsp/common/common.jsp"/>
<link href="${staticMemberPath}/new/css/host_addbank.css" rel="stylesheet" type="text/css"/>
<link href="${staticMemberPath}/css/ucfgroup-nicevalidator-extends.css" rel="stylesheet">
<script src="${staticMemberPath}/js/ucfgroup-nicevalidator-extends.js" type="text/javascript"></script>
<script src="${basePath}/passwordControl/index.js" type="text/javascript"></script>
<script type="text/javascript">
	function myReload(){
		document.getElementById("J-checkcode-img").src=document.getElementById("J-checkcode-img").src + "?nocache="+new Date().getTime();
	}
</script>
<style type="text/css">
p.tip span {
	font-weight:130px;
	background-color: yellow;
	color:red;
}
p.tipp span {
	font-weight:130px;
	background-color: yellow;
	color:red;
}
p.tipp {
	font-weight:130px;
	background-color: yellow;
	color:red;
}
p.tippp span {
	font-weight:130px;
	background-color: yellow;
	color:red;
}
p.tipp span {font-weight:130px;
	background-color: yellow;
	color:red;
}
</style>
</head>
<body>
<c:import url="/common/head.htm"></c:import>
<div class="person-navigation">
	<ul class="container">
		<li><a href="${basePath}/user/basicinfo.htm">信息管理</a></li>
		<li><a href="${basePath}/bank/index.htm">银行卡管理</a></li>
	</ul>
	<div class="clear"></div>
</div>
<section class="bgc-gray">
		<div class="box">
			<div class="main background-wite ps-md-passw">
				<div class="box_title pl20"><em class="f18">删除银行卡</em></div>
				<div class="h_border_b">
					<table>
						<colgroup>
							<col width="155">
							<col width="745">
						</colgroup>
						<tbody class="h_open_tab f14">
							<tr>
								<th>姓名：</th>
								<td>${userName }</td>
							</tr>
							<tr>
								<th>身份证： </th>
								<td>${certNo }</td>
							</tr>
						</tbody>
					</table>
				</div>
				<form id="deleteBankCardFrom" name="deleteBankCardFrom" style="margin-left:150px;" action="${basePath}/bank/delete.htm" method="post" data-validator-option="{theme:'ucfgroup_effect',timely:2}">
					<input type="hidden" name="payPassword"/>
					<p><label for="name">银行卡号 :</label>${cardNo}&nbsp;</p>
					<p><label for="name">开户行地址 :</label>${province }   ${city }   ${branchname }&nbsp;</p>
					<p><label for="name">手机号码 :</label>${mobile}&nbsp;</p>
					<p><label for="name">短信验证码 :</label>
						<input type="text" id="vercode" name="vercode" maxlength="6" data-tip="输入手机6位验证码，如：88866" style=" vertical-align: middle;margin-bottom:2px;width:105px;"/>
						<input id="validPhoneCode" type="button" onclick="sendSMSNew(${cell},1);" style="height: 40px; font-size: 12px;padding-left:0px;width: 105px;color:#666666; margin-left:5px;" value="获取验证码">
					</p>
					<p style="margin-top:20px;"><label for="name" style="float:left;">支付密码 :</label>
						<jsp:include page="../p2p/passwordControl/resource_pwd.jsp" />
					</p>
					<span style="display:inline-block; margin-left:123px; color:#f85048;">${msg}</span>
					</br>
					</br>
					<p style="margin:25px 0 0 125px;">
						<a class="btn" onclick="Form_Submit('deleteBankCardFrom');" style="margin-right:7px;" href="javascript:void(0)">确 定</a>
						<a class="btn" style="color:#666666;background:#f3f3f3;border:solid 1px #cccccc;" href="${basePath}/bank/index.htm">返 回</a>
					</p>
				</form>
			</div>
		</div>
	</section>
<c:import url="/WEB-INF/views/jsp/common/newfooter.jsp"></c:import>
<jsp:include page="../common/resource_js.jsp"/>
<script type="text/javascript"> 
$(document).ready(function() {
	$('#deleteBankCardFrom').validator({
		stopOnError: true,
		timely: true,
		fields: {
			'vercode':'required;length[6];',
			'paypwd':'required;'
		}
	});
});
</script>
<script src="${staticMemberPath}/js/jquery.validator.js" type="text/javascript"></script>
<script src="${staticMemberPath}/js/zh_CN.js" type="text/javascript"></script>
<script src="${staticMemberPath}/js/ucfgroup-nicevalidator-extends.js" type="text/javascript"></script>
</body>
</html>