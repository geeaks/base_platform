package com.ucf.customer.utils.enums;

public enum EnumSessionKey {
	
	

	MEMBER_AGENT_KEY("session_memberAgent","登录用户验证信息"),
	USER_KEY("session_user","登录用户信息"),
	IMG_CODE_KEY("session_img_code","图片验证码"),
	SMS_PHONE("session_sms_phone","短信发送手机号"),
	FIND_LGNPWD_USER("session_find_lgnPwd_user","登录用户验证信息");
	
	
	private EnumSessionKey(String key,String msg){
		this.key = key;
		this.msg = msg;
	}
	private String key;
	
	private String msg;
	
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	
	
	
}
