<div class="container">
	<div class="login-all">
	<form action="${basePath}/login/login.htm" id="loginForm" name="loginForm" method="post">
		<div class="login-body">
			<input type="hidden" name="backUrl" value="${backUrl }" />
			<input type="hidden" name="redirectUrl" value="${redirectUrl }" />
			<div id="log-title" class="login-title row">
				登录先锋支付
			</div>
			<div id="msg_holder" style="margin-left:-8px;color: #000;">
				<span class="msg-box n-right">
					<c:if test="${msg!=null}">
						<div>
							<p class="msg_holder_red">
								${msg}
							</p>
						</div>
					</c:if>
					<c:if test="${usg!=null}">
						<div>
							<p class="msg_holder_red">
								${usg}
							</p>
						</div>
					</c:if>
					<c:if test="${lsg!=null}">
						<div>
							<p class="msg_holder_red">
								${lsg}
							</p>
						</div>
					</c:if>
				</span>
			</div>
			<div style="height:40px;" >
				<span class="name-img"><img src="${staticMemberPath}/images/user.jpg"></img></span>
				<input type="text" value="${user.cell}" id="username" name="username" class="input-block-level " maxlength="11" placeholder=" 用户名">
			</div>
			<div style="height: 40px;">
				<span class="pass-word-img add-mgt10"><img src="${staticMemberPath}/images/password.jpg"></img></span>
				<jsp:include page="../../jsp/common/passwordControl/user/resource_pwd_login.jsp" />
			</div>
               
                    <div class="row" style="height: 40px;">
                    	<div class="col-md-6" style="padding:0">
		                    <span class="pass-word-img add-mgt10">
		                    	<img src="${staticMemberPath}/images/text.jpg"/>
		                    </span>
		                    <input type="text" id="checkCode" name="checkCode"   maxlength="4"  class="input-block-level wx add-mgt10" placeholder=" 验证码">
	                    </div>
	                    <div class="col-md-6" style="padding:0;margin-bottom:15px">
	                     	<img id="J-checkcode-img" title="点击图片刷新验证码"	onclick="myReload()" class="checkcode-img add-mgt10" complete="complete" src="${basePath}/idcode.do" alt="输入验证码" style="cursor: pointer;height:38px;width:105px;float:right;"	seed="JIndexForm-J-checkcode-img" smartracker="on"/> 
	                  	</div>
                    </div>
                    <div class="row" style="padding:0;">
	        			<button  style="height: 40px; margin-top:0" type="button" onclick="Form_Submit('loginForm')" class="btn btn-large btn-primary sign-in-btn" >登 录</button>
                    </div>
                    <span class="sign-in"><a href="${basePath}/reg/index.htm">免费注册</a></span>
                    <span class="forget"><a href="${basePath}/lgnpwd/findPwd.htm">忘记密码？</a></span>
                </div>
            </form>
