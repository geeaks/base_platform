	<script type="text/javascript">
	$(document).ready(function() {
		$('#loginForm').validator({
			 timely: true,
			 stopOnError: false,
			 rules: {
				 tel: [/^1\d{10}$/, '请输入正确的手机号']
			},
			fields: {
				'username':'手机号:required;length[11];tel;',
				'pwdVali':'登录密码:required;',
				'checkCode':'验证码:required;length[4];remote[${basePath}/checkIdcode.do, checkCode];'
			}
			,
			 invalid: function(form, errors){
			         var html = '';
			         if (null!=errors&&errors.length>0) {
			        	html += '<span class="msg-box n-right" ><div><p class="msg_holder_red">'+errors[0]+'</p></div></span>'
			        }
			       $('#log-title').html(html);
			    }
			
		});
		$('#checkCode').keyup(function(event){
			var v = $(this).val();
			if(v!=null && v.length==4){
				$(this).trigger("validate");
			}
			if(event.keyCode == 13){
				Form_Submit('loginForm');
			}
		});
	});
	
	function myReload(){
		document.getElementById("J-checkcode-img").src=document.getElementById("J-checkcode-img").src + "?nocache="+new Date().getTime();
	}
	</script>
	<style type="text/css">
	.msg-wrap {
		display: none;
