package com.ucf.customer.utils.enums;

public enum EnumResultCode {

	SUCCESS("00","成功"),
	ING("01","处理中"),
	FAI("02","失败"),
	ERROR("03","系统异常"),

	NO_EXIST("04","不存在"),
	EXIST("05","已存在"),
	REPEAT("06","重复"),

	ERROR_DB("91","数据库异常");
	
	private String code;
	private String msg;
	
	EnumResultCode(String code, String msg) {
		this.code = code;
		this.msg = msg;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

}
