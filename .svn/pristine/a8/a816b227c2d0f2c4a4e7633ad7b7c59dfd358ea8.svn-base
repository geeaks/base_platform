	-->
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>
	<script type="text/javascript" src="../js/com.ucfgroup.select.province.js"></script>
	<script type="text/javascript">
	<!--
	$(function(){
		var cselectInst = new CascadeSelect('select-province','select-city', 'select-town');
	});
	//-->
