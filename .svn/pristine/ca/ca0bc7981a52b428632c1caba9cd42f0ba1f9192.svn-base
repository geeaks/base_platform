public enum EnumFieldType {
	NOT_NULL("不为空", 1);

	private String name;
	private int code;

	EnumFieldType(String name, int code) {
		this.name = name;
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	public static EnumFieldType getByCode(int code) {
		for (EnumFieldType enunEnumbase_platformUserType : EnumFieldType
				.values()) {
			if (enunEnumbase_platformUserType.code == code) {
				return enunEnumbase_platformUserType;
			}
		}
		return null;
	}
}
