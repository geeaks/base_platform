package com.ucf.customer.api.enums;

/**
 * @Description: 照片审核类型
 * @ClassName: EnumPhotoAuditType
 * @author gaoxiang
 * @date 2015-3-16 下午1:38:53
 */ 
public enum EnumPhotoAuditType {
	
	IDCARD_AUTH("IDCARD", "身份审核"), 
	IDCARD_BANKCARD_AUTH("IDCARD_BANKCARD", "身份证、银行卡审核"), 
	LIMIT_OPT_AUTH("LIMIT_OPT", "提升额度审核");
	
	private String code;
	private String msg;
	
	EnumPhotoAuditType(String code, String msg) {
		this.code = code;
		this.msg = msg;
	}
	
	public String getCode() {
		return code;
	}
	
	public void setCode(String code) {
		this.code = code;
	}
	
	public String getMsg() {
		return msg;
	}
	
	public void setMsg(String msg) {
		this.msg = msg;
	}
	
}
