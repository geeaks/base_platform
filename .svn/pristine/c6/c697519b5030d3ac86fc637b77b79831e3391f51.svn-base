package com.ucf.customer.api.enums;

/**
 * 银行卡类型
 * @author zhanglei
 *
 */
public enum EnumBankCardType {
	
	//借记卡
	DEBIT_CARD("1","DC","借记卡"),
	//贷记卡
	CREDIT_CARD("2","CC","贷记卡"),
	//金融网关状态  银行卡类型
	//准贷记卡
	SEMI_CREDIT_CARD("3","SC","准贷记卡"),
	//预付费卡
	PREPAID_CARD("4","PC","预付费卡"),
	//对公账户
	BUSINESS_ACCOUNT("BA","--","对公账户");
	
	
	private EnumBankCardType(String code,String fcode,String name) {
		this.code = code;
		this.fcode = fcode;
		this.name = name;
	}

	/** 代码 */
	private String code;
	/** 金融网关 code*/
	private String fcode;
	/** 名称 */
	private String name;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}
	
	public String getFcode() {
		return fcode;
	}

	public void setFcode(String fcode) {
		this.fcode = fcode;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
