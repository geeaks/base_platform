package com.ucf.customer.pojo;

import java.io.Serializable;
import java.util.Date;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

public class UcfBankCard implements Serializable {

	private static final long serialVersionUID = -758200743482920936L;

	@SensitiveInfo(type=SensitiveType.BANK_CARD)
	private String cardNo;
	@SensitiveInfo(type=SensitiveType.CHINESE_NAME)
	private String cardName;

	private String userId;

	private String bankId;

	private String bankName;

	private String cardType;
	
	@SensitiveInfo(type=SensitiveType.ALL)
	private String paymentPassword;

	private String registerFrom;

	private String certFrom;

	/**
	 * 卡状态：默认提现状态： 0，提现卡新增：1，提现卡删除：3 ，P2P快捷充值卡：2
	 */
	private String certStatus;

	private Date gmtCreate;

	private Date gmtModified;

	private String memo;

	private Date lastPayDt;

	private String certifyInfo;

	private String province;

	private String city;

	private String branchname;

	private String id;

	@SensitiveInfo(type=SensitiveType.CNAPS_CODE)
	private String issuer;

	private String confirmStatus;
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String phone;

	private String status;

	private long unionSeq;
	
	/**
	 * 信用卡CVN2
	 */
	@SensitiveInfo(type=SensitiveType.ALL)
	private String cvn2;

	/**
	 * 信用卡有效期
	 */
	@SensitiveInfo(type=SensitiveType.ALL)
	private String validDate;
	
	/**
	 * 支付标识 : Y N
	 */
	private String isPay;
	/**
	 * 提现标识 : Y N
	 */
	private String isWithdraw;
	/**
	 * 删除标识 : Y N
	 */
	private String isDelete;
	
	/**
	 * @Fields useType : 使用类型
	 */ 
	private String useType;
	
	public String getUseType() {
		return useType;
	}

	public void setUseType(String useType) {
		this.useType = useType;
	}

	public String getConfirmStatus() {
		return confirmStatus;
	}

	public void setConfirmStatus(String confirmStatus) {
		this.confirmStatus = confirmStatus;
	}

	public String getIssuer() {
		return issuer;
	}

	public void setIssuer(String issuer) {
		this.issuer = issuer;
	}

	public void setCardNo(String cardNo) {
		this.cardNo = cardNo;
	}

	public String getCardNo() {
		return cardNo;
	}

	public String getCardName() {
		return cardName;
	}

	public void setCardName(String cardName) {
		this.cardName = cardName;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getUserId() {
		return userId;
	}

	public void setBankId(String bankId) {
		this.bankId = bankId;
	}

	public String getBankId() {
		return bankId;
	}

	public void setBankName(String bankName) {
		this.bankName = bankName;
	}

	public String getBankName() {
		return bankName;
	}

	public void setCardType(String cardType) {
		this.cardType = cardType;
	}

	public String getCardType() {
		return cardType;
	}

	public void setPaymentPassword(String paymentPassword) {
		this.paymentPassword = paymentPassword;
	}

	public String getPaymentPassword() {
		return paymentPassword;
	}

	public void setRegisterFrom(String registerFrom) {
		this.registerFrom = registerFrom;
	}

	public String getRegisterFrom() {
		return registerFrom;
	}

	public void setCertFrom(String certFrom) {
		this.certFrom = certFrom;
	}

	public String getCertFrom() {
		return certFrom;
	}

	public void setCertStatus(String certStatus) {
		this.certStatus = certStatus;
	}

	public String getCertStatus() {
		return certStatus;
	}

	public void setGmtCreate(Date gmtCreate) {
		this.gmtCreate = gmtCreate;
	}

	public Date getGmtCreate() {
		return gmtCreate;
	}

	public void setGmtModified(Date gmtModified) {
		this.gmtModified = gmtModified;
	}

	public Date getGmtModified() {
		return gmtModified;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

	public String getMemo() {
		return memo;
	}

	public void setLastPayDt(Date lastPayDt) {
		this.lastPayDt = lastPayDt;
	}

	public Date getLastPayDt() {
		return lastPayDt;
	}

	public void setCertifyInfo(String certifyInfo) {
		this.certifyInfo = certifyInfo;
	}

	public String getCertifyInfo() {
		return certifyInfo;
	}

	public void setProvince(String province) {
		this.province = province;
	}

	public String getProvince() {
		return province;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getCity() {
		return city;
	}

	public void setBranchname(String branchname) {
		this.branchname = branchname;
	}

	public String getBranchname() {
		return branchname;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public long getUnionSeq() {
		return unionSeq;
	}

	public void setUnionSeq(long unionSeq) {
		this.unionSeq = unionSeq;
	}

	public String getCvn2() {
		return cvn2;
	}

	public void setCvn2(String cvn2) {
		this.cvn2 = cvn2;
	}

	public String getValidDate() {
		return validDate;
	}

	public void setValidDate(String validDate) {
		this.validDate = validDate;
	}

	public String getIsPay() {
		return isPay;
	}

	public void setIsPay(String isPay) {
		this.isPay = isPay;
	}

	public String getIsWithdraw() {
		return isWithdraw;
	}

	public void setIsWithdraw(String isWithdraw) {
		this.isWithdraw = isWithdraw;
	}

	public String getIsDelete() {
		return isDelete;
	}

	public void setIsDelete(String isDelete) {
		this.isDelete = isDelete;
	}

	@Override
	public String toString() {
		return "UcfBankCard [cardNo=" + cardNo + ", userId=" + userId
				+ ", bankId=" + bankId + ", bankName=" + bankName
				+ ", cardType=" + cardType + ", paymentPassword="
				+ paymentPassword + ", registerFrom=" + registerFrom
				+ ", certFrom=" + certFrom + ", certStatus=" + certStatus
				+ ", gmtCreate=" + gmtCreate + ", gmtModified=" + gmtModified
				+ ", memo=" + memo + ", lastPayDt=" + lastPayDt
				+ ", certifyInfo=" + certifyInfo + ", province=" + province
				+ ", city=" + city + ", branchname=" + branchname + ", id="
				+ id + ", issuer=" + issuer + ", confirmStatus="
				+ confirmStatus + ", phone=" + phone + "status" + status + "]";
	}

}
