package com.ucf.customer.fundpay.model;

import java.io.Serializable;

import javax.validation.constraints.NotNull;


/**
 * @ClassName: RegisterBo
 * @Description: 基金平台 注册业务实体类
 * @author gaoxiang
 * @date 2014年9月22日 上午11:50:50
 */ 
public class RegisterBo implements Serializable {
	
	/**
	 * @Fields serialVersionUID : 序列化id
	 */ 
	private static final long serialVersionUID = 7114024688851558695L;
	/**
	 * @Fields lgnPwd : 登录名 手机号
	 */ 
	@NotNull
	private String userName;
	/**
	 * @Fields lgnPwd : 登录密码
	 */ 
	@NotNull
	private String lgnPwd;
	/**
	 * @Fields paymentPassword : 支付密码
	 */
	@NotNull
	private String paymentPassword;
	/**
	 * @Fields realName : 真实姓名
	 */ 
	@NotNull
	private String realName;
	/**
	 * @Fields certNo : 证件号码
	 */ 
	@NotNull
	private String certNo;
	/**
	 * @Fields certEffectiveYear : 证件到期年
	 */ 
	@NotNull
	private String certEffectiveYear;
	/**
	 * @Fields certEffectiveMonth : 证件到期月
	 */ 
	@NotNull
	private String certEffectiveMonth;
	/**
	 * @Fields quesInfo : 密保问题
	 */ 
	@NotNull
	private String quesInfo;
	/**
	 * @Fields quesRes : 密保答案
	 */ 
	@NotNull
	private String quesRes;
	
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getLgnPwd() {
		return lgnPwd;
	}
	public void setLgnPwd(String lgnPwd) {
		this.lgnPwd = lgnPwd;
	}
	public String getPaymentPassword() {
		return paymentPassword;
	}
	public void setPaymentPassword(String paymentPassword) {
		this.paymentPassword = paymentPassword;
	}
	public String getRealName() {
		return realName;
	}
	public void setRealName(String realName) {
		this.realName = realName;
	}
	public String getCertNo() {
		return certNo;
	}
	public void setCertNo(String certNo) {
		this.certNo = certNo;
	}
	public String getCertEffectiveYear() {
		return certEffectiveYear;
	}
	public void setCertEffectiveYear(String certEffectiveYear) {
		this.certEffectiveYear = certEffectiveYear;
	}
	public String getCertEffectiveMonth() {
		return certEffectiveMonth;
	}
	public void setCertEffectiveMonth(String certEffectiveMonth) {
		this.certEffectiveMonth = certEffectiveMonth;
	}
	public String getQuesInfo() {
		return quesInfo;
	}
	public void setQuesInfo(String quesInfo) {
		this.quesInfo = quesInfo;
	}
	public String getQuesRes() {
		return quesRes;
	}
	public void setQuesRes(String quesRes) {
		this.quesRes = quesRes;
	}
	@Override
	public String toString() {
		return "RegisterBo [userName=" + userName + ", lgnPwd=" + lgnPwd
				+ ", paymentPassword=" + paymentPassword + ", realName="
				+ realName + ", certNo=" + certNo + ", certEffectiveYear="
				+ certEffectiveYear + ", certEffectiveMonth="
				+ certEffectiveMonth + ", quesInfo=" + quesInfo + ", quesRes="
				+ quesRes + "]";
	}
	
}
