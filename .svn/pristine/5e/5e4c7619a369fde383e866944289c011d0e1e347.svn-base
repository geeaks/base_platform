public enum EnumPasswordType {
	LOGIN_PASSWORD("lgoin", "登录密码"), PAY_PASSWORD("pay", "支付密码");
	private String code;
	private String desc;

	EnumPasswordType(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
}
