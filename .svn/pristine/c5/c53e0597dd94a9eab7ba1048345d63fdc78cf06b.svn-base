package com.ucf.customer.api.enums;

public enum EnumBankCardUseType {
	
	FINANCIAL("F","理财卡"),CONSUME("C","消费卡"),SETTLE("S","结算卡");
	
	private String code;
	private String msg;
	
	private EnumBankCardUseType(String code, String msg) {
		this.code = code;
		this.msg = msg;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}
	
}
