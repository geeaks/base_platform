<%@ page contentType="text/html;charset=UTF-8" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>先锋支付--会员</title>
<jsp:include page="/WEB-INF/views/jsp/common/common.jsp"/>
<link href="${staticMemberPath}/new/css/host_open.css" rel="stylesheet" type="text/css" />
<link href="${staticMemberPath}/css/bootstrap.css" rel="stylesheet">
</head>
<body>
<c:import url="/common/head.htm"></c:import>
<section class="bgc-gray">
	<div class="box">
		<div class="main">
			<c:choose>
				<c:when test="${flag==true}">
					<div class="find-failure">
						${msg}
					</div>
				</c:when>
				<c:otherwise>
					<div class="find-success">
						<!-- 恭喜您，设置成功 ！请牢记新的支付密码 -->
						${msg}
					</div>
				</c:otherwise>
			</c:choose>
			<div class="find-success-down">
				<a href="${basePath}/user/index.htm">返回先锋支付首页</a>
			</div>
		</div>
	</div>
</section>
<c:import url="/WEB-INF/views/jsp/common/newfooter.jsp"></c:import>
<jsp:include page="./resource_js.jsp"/>
</body>
</html>