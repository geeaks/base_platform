# redis configration
redis.pool.host=${redis.host.ip}
redis.pool.port=${redis.host.port}
redis.pool.maxIdle=5000
redis.pool.maxTotal=5000
redis.pool.timeBetweenEvictionRunsMillis=30000
redis.pool.minEvictableIdleTimeMillis=30000
redis.pool.testOnBorrow=true
redis.encode=utf-8
redis.expire=604800000
redis.unlock=false

#redis-session
redis.session.shareded=false
redis.session.server=${redis.host.ip}:${redis.host.port}

#distribute lock
lock.redis.ip=${lock.redis.ip}
lock.redis.port=${lock.redis.port}

#redis-cluster
redis.server=${redis.server}
redis.server.read=${redis.server.read}
redis.shareded=${redis.shareded}

schedule.redis.ip=${schedule.redis.ip}
schedule.redis.port=${schedule.redis.port}
static.res.path=${static.res.path}
protection.system.host.ip=${protection.system.host.ip}
protection.system.host.port=${protection.system.host.port}

