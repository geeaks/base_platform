<%@ page contentType="text/html;charset=UTF-8" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<c:set var="basePath" value="${pageContext.request.contextPath }" /> 
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>先锋支付--修改用户身份证信息</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <jsp:include page="../common/resource_meta.jsp"/>
<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<c:set var="basePath" value="${pageContext.request.contextPath }" />
<jsp:include page="../common/resource_css.jsp"/>
<!--private js&css start-->
    <script type="text/javascript" src="${staticMemberPath}/new/js/host_findpassword.js"></script>
    <link href="${staticMemberPath}/new/css/set-pay-password.css" rel="stylesheet" type="text/css" />
    <link href="${staticMemberPath}/new/css/host_newuser.css" rel="stylesheet" type="text/css">
    <!--private js&css end-->
  </head>
  <body>
     <c:import url="/common/head.htm"></c:import>
<section>
        <div class="box">
        <div class="fd-main">
            <div class="fd-center">
               <div class="fd-layout">
                    <form action="${basePath}/setId/submit/" method="post">
                    <table width="100%">
                        <colgroup>
                            <col width="145" />
                            <col width="755" />
                        </colgroup>
                        <tbody>
                            
                            <tr>
                                <th>
                                    设置身份信息
                                </th>
                                <td>
                                   
                                </td>
                            </tr>
                            <tr>
                                <th>
                                   真实姓名：   
                                </th>
                                <td>
                                    <input type="text" id="realName" name="realName" class="idbox w228" placeholder="请输入您的真实姓名" />
                                </td>
                            </tr>
                            <tr>
                                <th>
                                    身份证号：
                                </th>
                                <td>
                                    <input type="text" id="certNo" name="certNo" class="idbox w228" placeholder="请输入您的身份证号码" />
                                </td>
                            </tr>
                            <tr>
                                <th>
                                    身份有效期：
                                </th>
                                <td>
                                    <select id="certEffectiveYear" name="certEffectiveYear" class="h_open_sel">
                                    <option value="2014">2014年</option>
                                    </select>
                                    /
                                    <select id="certEffectiveDate" name="certEffectiveDate" class="h_open_sel">
                                    <option value="3">3月</option>
                                    </select>
                                </td>
                            </tr>
                            <tr>
                                <th>
                                </th>
                                <td>
                                    <input type="submit" value="确定" class="but-yellow but-gray " />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    </form>
                </div>	
            </div>
        </div>
        </div>
    </section>
    

    <c:import url="/common/footer.htm"></c:import>
	
	
	<jsp:include page="../common/resource_js.jsp"/>
  </body>
</html>