<%@ page contentType="text/html;charset=UTF-8"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>先锋支付--已认证</title>
<jsp:include page="/WEB-INF/views/jsp/common/common.jsp"/>
<link href="${staticMemberPath}/new/css/set-pay-password.css" rel="stylesheet" type="text/css" />
<link href="${staticMemberPath}/new/css/host_newuser.css" rel="stylesheet" type="text/css" />
<link href="${staticMemberPath}/css/ucfgroup-nicevalidator-extends.css" rel="stylesheet">
<script src="${basePath}/passwordControl/index.js" type="text/javascript"></script>
<script src="${staticMemberPath}/js/jquery.validator.js" type="text/javascript"></script>
<style>
	p label{
		font-weight:400;
	}
	p input{
		height:40px !important;
	}	
</style>
</head>
<body>
<c:set var="tab" value="4" scope="application"/>
	<c:import url="/common/head.htm"></c:import>
	<div class="person-navigation">
		<ul class="container">
			<li class=""><a href="${basePath}/pwd/manage.htm">密码管理</a></li>
			<%-- <li class="person-navigation-active"><a href="${basePath}/bank/firstaddbank.htm">实名认证</a></li> --%>
		</ul>
		<div class="clear"></div>
	</div>
		<div class="box mt20">
			<div class="main background-wite ps-md-passw">
				<div class="box_title pl20"><em class="f18">实名认证</em></div>
				<div class="real-name">已经通过先锋支付实名认证</div>
				<div class="standing">您的身份信息</div>
				<form style="margin-left:380px;margin-top:20px;" action="${basePath}/pwd/pwdmodify.htm" id="pwdModForm" name="pwdModForm" method="post" data-validator-option="{theme:'ucfgroup_effect',timely:2}">
				<input type="hidden" name="payPassword"/>
					<p><label for="name" style="float:left;">真实姓名：</label>
						${realName}
					</p>
					<p><label for="name">身份证号：</label>
						${certNo}
					</p>
					<p><label for="name">身份证有效期：</label>
						${fn:substring(certValidEndDate, 0, 4)}年${fn:substring(certValidEndDate, 5, 7)}月
					</p>
					</br>
				</form>
			</div>
		</div>
	</section>
	<c:import url="/WEB-INF/views/jsp/common/newfooter.jsp"></c:import>
	<jsp:include page="../common/resource_js.jsp" />
</body>
<script type="text/javascript">
$(document).ready(function() {
});
</script>
<script src="${staticMemberPath}/js/jquery.validator.js" type="text/javascript"></script>
<script src="${staticMemberPath}/js/zh_CN.js" type="text/javascript"></script>
<script src="${staticMemberPath}/js/ucfgroup-nicevalidator-extends.js" type="text/javascript"></script>
</html>