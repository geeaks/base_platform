package com.ucf.customer.service;

import javax.security.auth.login.AccountException;
import com.ucf.customer.pojo.UcfBankCard;
import com.ucf.customer.pojo.UcfCard;
import com.ucf.customer.pojo.UcfCustomer;
import com.ucf.customer.pojo.UcfUser;


/**
 * @Description: MQ服务接口
 * @ClassName: MQConsumeService
 * @author gaoxiang
 * @date 2015-1-22 下午3:26:29
 */ 
public interface MQConsumeService {
	
	/**
	 * @Description: 更新银行卡
	 * @param userId
	 * @param bankAccountNo
	 * @param province
	 * @param city
	 * @param issuer
	 * @param branchName
	 * @throws Exception
	 * @return void 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:26:44
	 */
	public void updateBankCard(String userId,String bankAccountNo,String province, String city, String issuer,String branchName) throws Exception;
	
	/**
	 * @Description: 绑定银行卡 可以绑定多张银行卡 如果存在 则更新该银行卡
	 * @param userId
	 * @param cardType
	 * @param bankAccountNo
	 * @param bankCode
	 * @param bankName
	 * @param mobileNo
	 * @param province
	 * @param city
	 * @param issuer
	 * @param branchName
	 * @param cvn2
	 * @param validDate
	 * @throws AccountException
	 * @throws Exception
	 * @return void 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:26:54
	 */
	public void addBankCard(String userId,String cardType,String bankAccountNo,String bankCode, 
			String bankName, final String mobileNo,String province, String city, String issuer, 
			String branchName, String cvn2,String validDate) throws Exception ;
	
	/**
	 * @Description: 实名认证落数据
	 * @param userName
	 * @param bankAccountNo
	 * @param certNo
	 * @param mobileNo
	 * @param ucfUser
	 * @param ucfCard
	 * @return void 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:27:04
	 */
	public void realNameMethod(final String userName, String bankAccountNo, final String certNo, 
			final String mobileNo, final UcfUser ucfUser, final UcfCard ucfCard);
	
	/**
	 * @Description: 设置银行卡信息
	 * @param userId
	 * @param cardType
	 * @param bankAccountNo
	 * @param bankCode
	 * @param bankName
	 * @param mobileNo
	 * @param province
	 * @param city
	 * @param issuer
	 * @param branchName
	 * @param cvn2
	 * @param validDate
	 * @throws AccountException
	 * @return UcfBankCard 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:27:39
	 */
	public UcfBankCard setBankInfo(String userId,String cardType,String bankAccountNo,
			String bankCode, String bankName, final String mobileNo,
			String province, String city, String issuer, String branchName,String cvn2,String validDate)
			throws AccountException;
	
	/**
	 * @Description: 用户实名设置
	 * @param userName
	 * @param certNo
	 * @param mobileNo
	 * @param ucfUser
	 * @param ucfCard
	 * @param customer
	 * @return void 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:27:59
	 */
	public void userRealNameSet(final String userName, final String certNo, final String mobileNo, final UcfUser ucfUser,
			final UcfCard ucfCard, final UcfCustomer customer);
	
	/**
	 * @Description: 正则验证
	 * @param regex
	 * @param str
	 * @return boolean 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:57:09
	 */
	public boolean match(String regex, String str);
	
	/**
	 * @Description: 钱包绑卡 只可绑定一张银行卡
	 * @throws AccountException
	 * @throws Exception
	 * @return void 返回类型
	 * @author gaoxiang
	 * @date 2015-1-22 下午3:58:27
	 */
	public void addWalletBankCard(String userId,String cardType,String bankAccountNo,String bankCode, 
			String bankName, final String mobileNo,String province, String city, String issuer, 
			String branchName, String cvn2,String validDate) throws Exception;
	
}
