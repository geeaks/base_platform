package com.ucf.customer.ws.bo;

import java.io.Serializable;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

public class P2PQueryUserinfoReturnValueBo implements Serializable{

	private static final long serialVersionUID = -8251581326835795651L;
	
	private String respCode;
	private String respMsg;
	@SensitiveInfo(type=SensitiveType.CHINESE_NAME)
	private String name;
	private String gender;
	private String certType;
	@SensitiveInfo(type=SensitiveType.ID_CARD)
	private String certNo;
	private boolean payPasswdSet;
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String phone;
	@SensitiveInfo(type=SensitiveType.EMAIL)
	private String email;
	private String status;
	private Long amount;
	public String getRespCode() {
		return respCode;
	}
	public void setRespCode(String respCode) {
		this.respCode = respCode;
	}
	public String getRespMsg() {
		return respMsg;
	}
	public void setRespMsg(String respMsg) {
		this.respMsg = respMsg;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	public String getCertType() {
		return certType;
	}
	public void setCertType(String certType) {
		this.certType = certType;
	}
	public String getCertNo() {
		return certNo;
	}
	public void setCertNo(String certNo) {
		this.certNo = certNo;
	}
	public boolean isPayPasswdSet() {
		return payPasswdSet;
	}
	public void setPayPasswdSet(boolean payPasswdSet) {
		this.payPasswdSet = payPasswdSet;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Long getAmount() {
		return amount;
	}
	public void setAmount(Long amount) {
		this.amount = amount;
	}
}
