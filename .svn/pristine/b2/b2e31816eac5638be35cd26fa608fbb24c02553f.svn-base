<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<c:set var="basePath" value="${pageContext.request.contextPath }" />




<input type="hidden" id="baseUrl" value="${basePath}" />

											<div id="myTip" style="width:200px;height:40px;border:1px solid #888888;background:#FFFFFF;display:none;" align="center" valign="middle">
												<a id='downLink' href="${staticMemberPath}/${myexe}" class="mylink">
													<font face="宋体" size="-1">点此下载控件</font>
												</a>
											</div>
											<div id="myObj" class='' style="display:block;float:left;"  align="left">
												<!-- windows版本密码控件  -->
												<% if(request.getHeader("User-Agent").contains("Mac OS")==false){ 
													//如果是mac系统页面不加载windows版本插件 %>
												<object id="userpasswd"  classid="CLSID:598BD8F9-CE1C-4b52-9C0E-6EA55E2371A7" tabIndex="2"
													width="200px"
													height="40px"
													codebase="${staticMemberPath}/${mycab}#Version=${myVersion}">
													<param name="width" value="200">
													<param name="height" value="40">	
													<param name="textsize" value="12"> 
													<param name="color" value="#000000"> 
													<param name="bgcolor" value="#FFFFFF">  
													<param name="bordercolor" value="#96C2F1">  
													<param name="MyTime" value="${serverTime}">
												<embed id="userpasswdNP" type="application/598BD8F9-CE1C-4b52-9C0E-6EA55E2371A7" color="#000000" bgcolor="#FFFFFF" bordercolor="#96C2F1" width="200px" height="40px" textsize="16px" MyTime="${serverTime}" tabIndex="2" ></embed>
												</object>
												<%} %>
												<!-- <span class="msg-box n-right" for="pwd" style="">
												<span class="msg-wrap n-error" role="alert">
												<span class="n-icon"></span>
												<span class="n-msg">不能为空</span>
												</span>
												</span> -->
											</div>
											<div id="mac" style="display:none" align="left">
												<!-- MAC版本密码控件  -->
												<% if(request.getHeader("User-Agent").contains("Mac OS")){ 
													//如果是mac系统,页面才加载mac版本插件 %>
												<embed id="macPasswd" width="200" height="40" type="application/598BD8F9-CE1C-4b52-9C0E-6EA55E2371A7" ></embed>	
										
												<% } %>
											</div>
											<input id="pwd" name="pwd" type="hidden" />
											<input id="len" name="len" type="hidden" />
											<input id="pcData" name="pcData" type="hidden" />
											<input id="isMac" name="isMac" type="hidden" />