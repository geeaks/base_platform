package com.ucf.customer.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.ucf.customer.dao.UcfUserLoginAppDao;
import com.ucf.customer.dao.base.BaseCustomerDao;
import com.ucf.customer.pojo.UcfUserLoginApp;
@Repository
public class UcfUserLoginAppDaoImpl extends BaseCustomerDao implements UcfUserLoginAppDao {

private static final long serialVersionUID = -1L;

	public void insertUcfUserLoginApp(UcfUserLoginApp ucfUserLoginApp)throws Exception{
		this.insert("com.ucf.customer.insertUcfUserLoginApp", ucfUserLoginApp);
	}

	public void updateUcfUserLoginApp(UcfUserLoginApp ucfUserLoginApp)throws Exception{
		this.update("com.ucf.customer.updateUcfUserLoginApp", ucfUserLoginApp);
	}

	public void deleteUcfUserLoginApp(String userId)throws Exception{
