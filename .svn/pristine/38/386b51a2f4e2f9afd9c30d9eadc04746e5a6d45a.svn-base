<select id="getUcfUserByUserId"  parameterType="java.lang.String" resultType="ucfUser" >
<![CDATA[
SELECT 
USER_ID as userId,CARD_NO as cardNo,
REAL_NAME as realName,GENDER as gender,CERT_TYPE as certType,
