package com.ucf.customer.ws.bo;

import java.io.Serializable;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

/**
 * 
 * @desc 调用webService接口获取用户余额
 * @version 1.0
 * @date 2014-3-25 14:01:06
 * @author yuchao@ucfgroup.com
 * @return
 * @throws Exception
 */
public class AccBalanceParamBO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4909263311126290687L;

	// ACCOUNT_NO 字符串 20 必输 账户号
	@SensitiveInfo(type=SensitiveType.BANK_CARD)
	private String account_no;

	// TRANS_CODE 字符串 5 必输 交易码-6000
	private String trans_code;

	// SUB_TRANS_CODE 字符串 15 必输 子交易码-6001
	private String sub_trans_code;

	/**
	 * 获取账户号
	 * 
	 * @param Void
	 * @return String
	 */
	public String getAccountNo() {
		return account_no;
	}

	/**
	 * 设置账户号
	 * 
	 * @param account_no
	 * @return Void
	 */
	public void setAccountNo(String account_no) {
		this.account_no = account_no;
	}

	/**
	 * 获取交易码-固定传值：6000
	 * 
	 * @return
	 */
	public String getTransCode() {
		return trans_code;
	}

	/**
	 * 设置交易码-固定传值：6000
	 * 
	 * @param Stirng
	 * @return
	 */
	public void setTransCode(String trans_code) {
		this.trans_code = trans_code;
	}

	/**
	 * 获取子交易码-固定传值：6001
	 * 
	 * @return void
	 */
	public String getSubTransCode() {
		return sub_trans_code;
	}

	/**
	 * 设置子交易码-固定传值：6001
	 * 
	 * @param sub_trans_code
	 * @return void
	 */
	public void setSubTransCode(String sub_trans_code) {
		this.sub_trans_code = sub_trans_code;
	}

}
