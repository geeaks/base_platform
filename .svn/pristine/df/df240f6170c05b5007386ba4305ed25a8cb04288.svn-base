public enum EnumAuthType {
	NAME_CERT("01", "姓名、身份证 "), 
	BANK_CELL_NO("02", "银行卡、银行预留手机号"), 
	NAME_CERT_BANK_CELL("03", "姓名、身份证、银行卡、银行预留手机号 ");

	private String code;
	private String msgS;

	EnumAuthType(String code, String msg) {
		this.code = code;
		this.msgS = msg;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsgS() {
		return msgS;
	}

	public void setMsgS(String msgS) {
		this.msgS = msgS;
	}

	public boolean isSuccess() {
		if (this.getCode().equals("01")) {
			return true;
		} else {
			return false;
		}
	}
}
