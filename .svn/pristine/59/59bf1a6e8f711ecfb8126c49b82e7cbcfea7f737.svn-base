<%@ page contentType="text/html;charset=UTF-8" %>
<!DOCTYPE html>
<jsp:include page="../p2p/commons/resource_meta.jsp"/>
<jsp:include page="/WEB-INF/views/jsp/common/common.jsp"/>
<html>
<head>
<title>先锋支付</title>
<jsp:include page="commons/resource_css.jsp"/>
<jsp:include page="commons/resource_js.jsp"/>
<c:set var="basePath" value="${pageContext.request.contextPath }" />
<%-- <c:if test="${'common' == p2p_type }">
<script type="text/javascript" src="${staticbase_platformPath}/p2p/js/edit-trader-password/index_common.js"></script>
</c:if>
<c:if test="${'common' != p2p_type }">
<script type="text/javascript" src="${staticbase_platformPath}/p2p/js/edit-trader-password/index.js"></script>
</c:if> --%>
<c:if test="${'common' == p2p_type }">
<script type="text/javascript" src="${basePath}/passwordControl/individual_index.js"></script>
<script type="text/javascript" src="${basePath}/passwordControl/individual_index1.js"></script>
<script type="text/javascript" src="${basePath}/passwordControl/individual_index2.js"></script>
</c:if>
<c:if test="${'common' != p2p_type }">
<script type="text/javascript" src="${basePath}/passwordControl/findpaypwd_index.js"></script>
<script type="text/javascript" src="${basePath}/passwordControl/findpaypwd_index1.js"></script>
<script type="text/javascript" src="${basePath}/passwordControl/findpaypwd_index2.js"></script>
</c:if>
<script type="text/javascript">
	$(function() {
		$("#postForm").validator({
			fields: {
				'vfyCode': '验证码: required;'
			}
		});
	});
	
	function submitForm(){
		if(confirmPwd2()){
			Form_Submit2("postForm");
		}
	}
</script>
</head>
<body>
<section>
	<div class="et-nav">
		<div class="host_title">资金托管平台<span>修改支付密码</span></div>
	</div>
	<div class="et-main">
		<div class="et-center">
			<form action="${basePath}/p2p/modifyPayPwd" id="postForm" method="post">
				<table width="100%" class="mt20">
					<colgroup>
						<col width="270">
						<col width="600">
					</colgroup>
					<tbody>
						<tr>
							<th>您的先锋支付账号：</th>
							<td>${cellNo}</td>
						</tr>
						<tr>
							<th>原支付密码：</th>
							<td>
								<div style="height:50px;line-height:50px">
									<jsp:include page="/WEB-INF/views/jsp/p2p/passwordControl/individual_pwd2.jsp" />
								</div>
						</tr>
						<tr>
							<th>新支付密码：</th>
							<td>
								<div style="height:50px;line-height:50px">
									<jsp:include page="/WEB-INF/views/jsp/p2p/passwordControl/individual_pwd.jsp" />
								</div>
							</td>
						</tr>
						<tr>
							<th> 新支付密码确认：</th>
							<td>
								<div style="height:50px;line-height:50px">
									<jsp:include page="/WEB-INF/views/jsp/p2p/passwordControl/individual_pwd1.jsp" />
								</div>
							</td>
						</tr>
						<tr>
							<th> 手机号：</th>
							<td>
								<label class="h_open_lab">${requestScope.HID_user["cell"] }</label>
								<input id="validPhoneCode" type="button" onclick="sendSMS(${cellNo },0,'cellNoP2P_RECHARGE_REQUEST_INF_customer');" class="send_vfycode h_open_ipt h_open_btn h_checkcode_btn ml10" value="发送短信验证码" />
								<i id="sendSMSTipI" class="formvalidError formvalidError1" style="display: none;"></i>
								<em id="sendSMSTipEm" class="f12"></em>
							</td>
						</tr>
						<tr>
							<th>短信验证码：</th>
							<td>
								<input name="merchantId" type="hidden" value="${merchantId}"/>
								<input name="userId" type="hidden" value="${requestScope.HID_user['userId'] }"/>
								<input type="text" class="idbox w123" placeholder="请输入短信验证码" id="vfyCode" name="vfyCode" maxlength="6" />
								<span class="msg-box" for="vfyCode"></span>
							</td>
						</tr>
				</tbody>
			</table>
			<div class="et_btn_login">
				<input id="submitBtn" onclick="javascript:submitForm();" href="javascript:void(0)" type="button" value="下一步" class="but-yellow but-gray " />
				<a href="javascript:windowclose();" class="but-blue but-gray but-ml" >取消</a>
			</div>
		</form>
	</div>
</div>
</section>
</body>
</html>