package com.ucf.customer.ws.bo;

import java.io.Serializable;

import com.ucf.customer.ws.enums.EnumFieldType;
import com.ucf.customer.ws.enums.FieldAccess;
import com.ucf.customer.ws.enums.FieldName;
import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;
/**
 * 
 * Description: 为P2P移动端后台提供注册开发的dubbo服务接口，传入参数
 * All Rights Reserved.
 * @version 1.0  2014-5-19 下午4:04:14  by xuhao@ucfgroup.com创建
 */
public class RegistParamBo implements Serializable{

	
	private static final long serialVersionUID = -5027299496557107517L;
	
	/**
	 * 平台商id
	 */
	@FieldName( "平台商id")
	@FieldAccess(EnumFieldType.NOT_NULL)
	private String merchantId;
	
	
	/**
	 * 手机号
	 */
	@FieldName( "手机号")
	@FieldAccess(EnumFieldType.NOT_NULL)
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String phoneNo;
	
	/**
	 * 用户类型 ：  参考枚举 EnumUserType
	 */
	@FieldName( "用户类型")
	@FieldAccess(EnumFieldType.NOT_NULL)
	private String userType;
	
	/**
	 * 真实姓名
	 */
	@FieldName( "真实姓名")
	@FieldAccess(EnumFieldType.NOT_NULL)
	@SensitiveInfo(type=SensitiveType.CHINESE_NAME)
	private String realName;
	
	
	/**
	 * 证件类型号（IDC身份证，其他支持类型等待确定） 参考 EnumIdCardType
	 */
	@FieldName( "证件类型")
	@FieldAccess(EnumFieldType.NOT_NULL)
	private String idType;
	
	
	/**
	 * 证件号
	 */
	@FieldName( "证件号")
	@FieldAccess(EnumFieldType.NOT_NULL)
	@SensitiveInfo(type=SensitiveType.ID_CARD)
	private String idNo;
	
	/**
	 * 1 web端  2 移动端
	 */
	private String source;
	
	/**
	 * @Fields realNameStatus : 实名认证状态
	 */ 
	private String realNameStatus;
	
	/**
	 * @Fields certStatus : 实名认证类型
	 */ 
	private String certStatus;
	
	public String getRealNameStatus() {
		return realNameStatus;
	}
	public void setRealNameStatus(String realNameStatus) {
		this.realNameStatus = realNameStatus;
	}
	public String getCertStatus() {
		return certStatus;
	}
	public void setCertStatus(String certStatus) {
		this.certStatus = certStatus;
	}
	public String getMerchantId() {
		return merchantId;
	}
	public void setMerchantId(String merchantId) {
		this.merchantId = merchantId;
	}

	
	public String getPhoneNo() {
		return phoneNo;
	}
	public void setPhoneNo(String phoneNo) {
		this.phoneNo = phoneNo;
	}
	public String getUserType() {
		return userType;
	}
	public void setUserType(String userType) {
		this.userType = userType;
	}
	public String getRealName() {
		return realName;
	}
	public void setRealName(String realName) {
		this.realName = realName;
	}
	public String getIdType() {
		return idType;
	}
	public void setIdType(String idType) {
		this.idType = idType;
	}
	public String getIdNo() {
		return idNo;
	}
	public void setIdNo(String idNo) {
		this.idNo = idNo;
	}
	public String getSource() {
		return source;
	}
	public void setSource(String source) {
		this.source = source;
	}
}
