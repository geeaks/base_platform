package com.ucf.customer.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.ucf.customer.dao.UcfUserHabitDao;
import com.ucf.customer.pojo.UcfUserHabit;
import com.ucf.customer.service.UserHabitService;

@Service("userHabitService")
public class UserHabitServiceImpl implements UserHabitService {
	
	@Autowired
	private UcfUserHabitDao userHabitDao;
	@Override
	public void save(UcfUserHabit habit) throws Exception {
		userHabitDao.insertUcfUserHabit(habit);

	}

	@Override
	public void modify(UcfUserHabit habit) throws Exception {
		userHabitDao.updateUcfUserHabit(habit);

	}

	@Override
	public List<UcfUserHabit> query(UcfUserHabit habit) throws Exception {
		
		return userHabitDao.queryUcfUserHabitListByWhere(habit);
	}

}
