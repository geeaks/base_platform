public class BankCardUtil {
	
	public static String cardNoEncrypt(String cardNo){
		if (null!=cardNo&&cardNo.length()>4)
			return cardNo.substring(0,4)+"********" + cardNo.substring(cardNo.length()-4, cardNo.length());
		return cardNo;
	}

}
