package com.ucf.customer.utils;

/**
 * 账务交易码
 * @version 1.0  2014-3-11 上午10:43:03  by qtw（qutianwei@ucfgroup.com）创建
 */
public enum TransCode {
	PAY("1000","1001"),PAY_FREEZZE("1000","1002"),
	WITHDRAW("2000","2001"),UNFREEZE_WITHDRAW("2000","2002"),
	TRANSFER("3000","3001"),UNFREEZE_TRANSFER("3000","3002"),TRANSFER_FREEZE("3000","3003"),UNFREEZE_TRANSFER_BAL("3000","3004"),
	FREEZE("4000","4001"),
	UNFREEZE("5000","5001"),
	SEARCH_ACCOUNT("6000","6001"),SEARCH_ACCOUNT_LOG("6000","6002"),SEARCH_ACCOUNT_FREEZE_LOG("6000","6003"),
	OPENACCOUNT("7000","7001"),
	REVERSE("8000","8001");//冲正
	
	private String transCode;
	private String subTransCode;
	
	private TransCode(String transCode,String subTransCode){
		this.transCode = transCode;
		this.subTransCode = subTransCode;
	}
	
	public String getTransCode(){
		return transCode;
	}
	
	public String getSubTransCode(){
		return subTransCode;
	}
}
