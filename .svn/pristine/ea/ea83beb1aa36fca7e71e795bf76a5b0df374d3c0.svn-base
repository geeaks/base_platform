package com.ucf.customer.api.enums;

public enum EnumUserType {

	ANONYMOUS("-1","匿名"),
	PERSON("1","个人"),
	ENTERPRISE("2","企业"),
	OPERATOR_TYPE("3", "操作员");
	
	private String code;
	private String desc;

	EnumUserType(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
}
