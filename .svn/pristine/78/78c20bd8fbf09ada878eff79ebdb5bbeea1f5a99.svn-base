    <script type="text/javascript">
        var APP_ROOT = '';
        var ERROR_IMG = '/static/v1/images/common/image_err.gif';
        var send_span = 60000;
        
        function sendSMSNew(cellNo,smsType) {
        	var param = {
        			cellNo: cellNo,
        			smsType : smsType
        		};
        	$.ajax({
        		type:'POST',
        		async:true,
        		timeout:30000,
        		url:'${basePath}/sms/send.htm',
        		data:param,
        		beforeSend:function() {
        			$('#validPhoneCode').attr("disabled","disabled");
        		},
        		error:function(){
        			$('#validPhoneCode').removeAttr("disabled");
        		},
        		success:function(result) {
        			freezeTime();
        		},
        		dataType:"json"
        	});
        }
    </script>
