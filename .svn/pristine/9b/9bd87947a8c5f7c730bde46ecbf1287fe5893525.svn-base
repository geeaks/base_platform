public enum EnumStatusType {
	IDC_TYPE("00", "代扣"), POLICE_TYPE("01", "公安部接口");
	private String code;
	private String desc;

	EnumStatusType(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	

}
