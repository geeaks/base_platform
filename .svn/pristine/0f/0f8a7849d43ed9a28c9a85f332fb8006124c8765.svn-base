public enum EnumAuthority {
	
	base_platform_LOGIN("登录权限",1);
	
	private String name;
	private int code;
	
	EnumAuthority(String name,int code){
		this.name = name;
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}
	
	

