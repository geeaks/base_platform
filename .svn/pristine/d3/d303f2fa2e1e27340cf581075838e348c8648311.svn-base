public enum EnumAccountType {
	SUCCESS("01", "成功"), ERROR("02", "失败");

	private String code;
	private String msgS;

	EnumAccountType(String code, String msg) {
		this.code = code;
		this.msgS = msg;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsgS() {
		return msgS;
	}

	public void setMsgS(String msgS) {
		this.msgS = msgS;
	}

	public boolean isSuccess() {
		if (this.getCode().equals("01")) {
			return true;
		} else {
			return false;
		}
	}
}
