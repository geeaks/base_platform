package com.ucf.customer.ws.bo;

import java.io.Serializable;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

/**
 * 
 * @desc 调用webService接口获取卡信息
 * @version 1.0
 * @date 2014-4-1 14:01:06
 * @author yuchao@ucfgroup.com
 * @return
 * @throws Exception
 */
public class CardInfoParamBO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -8317839996808796327L;

	// CARD_NO 字符串 20 必输 卡号
	@SensitiveInfo(type=SensitiveType.BANK_CARD)
	private String card_no;

	// CARD_TYPE 字符串 5 必输 卡类型
	private String card_type;

	/**
	 * 获取账户卡号码
	 * 
	 * @param Void
	 * @return String
	 */
	public String getCardNo() {
		return card_no;
	}

	/**
	 * 设置账户卡号码
	 * 
	 * @param card_no
	 * @return Void
	 */
	public void setCardNo(String card_no) {
		this.card_no = card_no;
	}

	/**
	 * 获取卡类型
	 * 
	 * @return
	 */
	public String getCardType() {
		return card_type;
	}

	/**
	 * 设置卡类型
	 * 
	 * @param Stirng
	 * @return
	 */
	public void setCardType(String card_type) {
		this.card_type = card_type;
	}

}
