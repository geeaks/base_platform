package com.ucf.customer.ws.enums;

public enum EnumBankCardType {
	DEBIT_CARD("1", "借记卡"), CREDIT_CARD("2", "贷记卡");

	private String code;
	private String msgS;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsgS() {
		return msgS;
	}

	public void setMsgS(String msgS) {
		this.msgS = msgS;
	}

	EnumBankCardType(String code, String msg) {
		this.code = code;
		this.msgS = msg;
	}

	public static boolean isExist(String code) {
		for (EnumBankCardType enumBankCardType : EnumBankCardType.values()) {
			if (code.equals(enumBankCardType.getCode())) {
				return true;
			}
		}
		return false;
	}
}
