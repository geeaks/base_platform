package com.ucf.customer.api.bo;

import java.io.Serializable;
import java.util.Date;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

public class OperatorBo implements Serializable {

	/**
	 * @Fields serialVersionUID : 序列号
	 */ 
	private static final long serialVersionUID = -1268375713096196315L;
	/**
	 * @Fields operatorId : 操作员Id
	 */ 
	private String operatorId;

	/**
	 * @Fields userId : 所属商户Id
	 */ 
	private String userId;

	/**
	 * @Fields realName : 姓名
	 */ 
	@SensitiveInfo(type=SensitiveType.CHINESE_NAME)
	private String realName;

	/**
	 * @Fields cell : 联系电话
	 */ 
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String cell;

	/**
	 * @Fields email : 邮箱 登录名
	 */ 
	@SensitiveInfo(type=SensitiveType.EMAIL)
	private String email;
	
	/**
	 * @Fields loginPwd : 登录密码
	 */ 
	@SensitiveInfo(type=SensitiveType.ALL)
	private String loginPwd;

	/**
	 * @Fields active : 激活状态
	 */ 
	private String active;
	
	/**
	 * @Fields roleId : 角色Id
	 */ 
	private String roleId;
	
	/**
	 * @Fields roleName : 角色名称
	 */ 
	private String roleName;

	/**
	 * @Fields gmtCreate : 创建时间
	 */ 
	private Date gmtCreate;
	
	/**
	 * @Fields gmtModify : 修改时间
	 */ 
	private Date gmtModify;
	
	public String getOperatorId() {
		return operatorId;
	}

	public void setOperatorId(String operatorId) {
		this.operatorId = operatorId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getRealName() {
		return realName;
	}

	public void setRealName(String realName) {
		this.realName = realName;
	}

	public String getCell() {
		return cell;
	}

	public void setCell(String cell) {
		this.cell = cell;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getLoginPwd() {
		return loginPwd;
	}

	public void setLoginPwd(String loginPwd) {
		this.loginPwd = loginPwd;
	}

	public String getActive() {
		return active;
	}

	public void setActive(String active) {
		this.active = active;
	}

	public String getRoleId() {
		return roleId;
	}

	public void setRoleId(String roleId) {
		this.roleId = roleId;
	}

	public String getRoleName() {
		return roleName;
	}

	public void setRoleName(String roleName) {
		this.roleName = roleName;
	}

	public Date getGmtCreate() {
		return gmtCreate;
	}

	public void setGmtCreate(Date gmtCreate) {
		this.gmtCreate = gmtCreate;
	}

	public Date getGmtModify() {
		return gmtModify;
	}

	public void setGmtModify(Date gmtModify) {
		this.gmtModify = gmtModify;
	}

	@Override
	public String toString() {
		return "UcfOperator [operatorId=" + operatorId + ", userId=" + userId
				+ ", realName=" + realName + ", cell=" + cell + ", email="
				+ email + ", loginPwd=" + loginPwd + ", active=" + active
				+ ", roleId=" + roleId + ", roleName=" + roleName
				+ ", gmtCreate=" + gmtCreate + ", gmtModify=" + gmtModify + "]";
	}
}
