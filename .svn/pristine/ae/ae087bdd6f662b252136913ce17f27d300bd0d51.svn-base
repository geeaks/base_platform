		this.bankCardNo = bankCardNo;
	}

	public String valid(ServiceInterfaceModel model, Class<?>... groups) {

		Set<ConstraintViolation<ServiceInterfaceModel>> constraintViolations = this
				.getValidator().validate(model, groups);

		for (ConstraintViolation<ServiceInterfaceModel> c : constraintViolations) {
			LOGGER.debug("参数错误," + c.getPropertyPath() + ":" + c.getMessage());
			return "参数错误," + c.getPropertyPath() + ":" + c.getMessage();
		}
		return null;
	}
