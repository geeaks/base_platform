public enum EnumIdCardType {
	CERT_NO("身份证", "IDC"),
	GAT_NO("港澳台身份证", "GAT"),
	MILIARY_NO("军官证", "MILIARY"),
	PASS_PORT_NO("护照", "PASS_PORT");

	private String name;
	private String code;

	EnumIdCardType(String name, String code) {
		this.name = name;
		this.code = code;
	}

	public String getName() {
		return name;
	}
	

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public void setName(String name) {
		this.name = name;
	}

	public static EnumIdCardType getByCode(String code) {
		for (EnumIdCardType enunEnumMemberUserType : EnumIdCardType.values()) {
			if (enunEnumMemberUserType.code.equals(code)) {
				return enunEnumMemberUserType;
			}
		}
		return null;
	}
}
