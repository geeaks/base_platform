package com.ucf.customer.ws.bo;

import java.io.Serializable;

/**
 * 用户登录返回对象
 * @author zhanglei
 *
 */
public class UserLoginReturnBO  implements Serializable {

	private static final long serialVersionUID = -6881581044237605843L;
	/**
	 * 用户ID
	 */
	private String userId;
	/**
	 * cookieVal值
	 */
	private String cookieVal;
	/**
	 * 返回结果   S 成功 F 失败
	 */
	private String result;
	/**
	 * 返回 信息
	 */
	private String message;
	
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getCookieVal() {
		return cookieVal;
	}
	public void setCookieVal(String cookieVal) {
		this.cookieVal = cookieVal;
	}
	public String getResult() {
		return result;
	}
	public void setResult(String result) {
		this.result = result;
	}
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
}
