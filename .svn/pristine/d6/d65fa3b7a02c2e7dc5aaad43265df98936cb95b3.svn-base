public enum Enumbase_platformUserType {
	Visitor("无权限控制", 1), Logined("登录用户有权", 2), Merchant("商户用户有权", 3);

	private String name;
	private int code;

	Enumbase_platformUserType(String name, int code) {
		this.name = name;
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	public static Enumbase_platformUserType getByCode(int code) {
		for (Enumbase_platformUserType enunEnumbase_platformUserType : Enumbase_platformUserType
				.values()) {
			if (enunEnumbase_platformUserType.code == code) {
				return enunEnumbase_platformUserType;
			}
		}
		return null;
	}
}
