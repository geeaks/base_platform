public enum EnumConfirmStatus {
	CONFIRM_YES("T", "已经实名认证"), CONFIRM_NO("W", "未实名认证");
	private String code;
	private String desc;

	EnumConfirmStatus(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

}
