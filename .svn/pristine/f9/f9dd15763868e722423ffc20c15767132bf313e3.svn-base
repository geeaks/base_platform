<%@ page contentType="text/html;charset=UTF-8" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>先锋支付--会员中心</title>
<jsp:include page="/WEB-INF/views/jsp/common/common.jsp"/>
<link href="${staticMemberPath}/css/bootstrap.css" rel="stylesheet"/>

</head>
<body>
<c:set var="tab" value="2" scope="application"/>
<c:set var="tab1" value="2" scope="application"/>
	<c:import url="/common/head.htm"></c:import>
    
    <div class="person-navigation">
		<ul class="container">
			<li class="person-navigation-active"><a href="${basePath}/user/basicinfo.htm">信息管理</a></li>
            <li><a href="${basePath}/bank/index.htm">银行卡管理</a></li>
		</ul>
		<div class="clear"></div>
	</div>
    
    
<section class="bgc-gray">
	
	
	<%-- <c:import url="/common/left.htm?index=3"></c:import>   --%> 
    
    
    <form id="formValid" action="">
	<div class="user_right">
        <div class="user_right_box basc-gry">
            <div class=" pb20">
					<ul class="info-navigation">
						<li class="bas-active"><a href="#">基本信息</a></li>
						<!--li><a href="#">其他信息</a></li-->
					</ul>
					<div class="info-basic">
						<h1>基本信息</h1>
						<img src="${staticMemberPath}/images/info-people.png" />
						<ul class="mgl40">
							<li>姓名：</li>
							<li>账户名：</li>
							<li>安全等级：</li>
							<li>手机号码：</li>
							<li>身份验证：</li>
							<li>身份证有效期：</li>
							<li>注册时间：</li>
						</ul>
						<ul>
							<li>${user.realName} <span class="bubb">普通账户</span></li>
							<li>${user.cell}</li>
							<li class="info-green">高</li>
							<li>${user.cellEncrypt }</li>
							<li>${user.certNoEncrypt} 
								<span class="bubb">
									<c:if test="${empty(user.certNo) }">
										<a href="${basePath}/setId">设置 </a>
									</c:if>
									已验证
								</span>
							</li>
							<li>
								<c:if test="${empty(user.certValidEndDate) }">
									未设置
								</c:if>
								<fmt:formatDate value='${user.certValidEndDate}' pattern="yyyy-MM-dd" />
								<c:if test="${empty(user.certValidEndDate) }">
									<a href="${basePath}/setId">设置 </a>
								</c:if>
							</li>
							<li>
								<fmt:formatDate value='${user.gmtSign}' pattern="yyyy-MM-dd HH:mm:ss" />
							</li>
						</ul>
							<c:choose>
								<c:when test="${realAuth == 'realAuth'}">
									<div class="approve-all">
										<div class="approve">实名认证</div>
										<p style="margin-top:10px;">提高信任等级，提升限额额度，交易更受信任</p>
									</div>
								</c:when>
								<c:otherwise>
									<div class="not-approve-all">
										<div class="not-approve fl">实名认证</div>
										<%-- <a style="float:left;display:inline-block;margin-left:10px;" href="${basePath}/bank/firstaddbank.htm">立即认证</a> --%>
										<p style="margin-top:10px; clear:both;">提高信任等级，提升限额额度，交易更受信任</p>
									</div>
								</c:otherwise>
							</c:choose>
					</div>
				
        </div>
        <div class="clear" style="clear:both;"></div>
            </div>
            
        </div>
    </div>
    </form>    
</div>
<div class="clear"></div>
</section>
<c:import url="/WEB-INF/views/jsp/common/newfooter.jsp"></c:import>
	

</body>
</html>