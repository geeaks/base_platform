package com.ucf.customer.api.enums;

public enum EnumRespStatusCode {
	SUCCESS("00", "成功"),
	ERROR("01", "失败"),
	PARAM_ERROR("02","参数错误"),
	
	NO_EXIST_USER("30","用户不存在"),
	EXIST_USER("31","用户已存在"),
	EXIST_MOBLEPHOE("32","手机号被占用"),
	NO_EXIST_SUBCARD("33","用户子卡不存在"),
	EXIST_SUBCARD("34","用户子卡已存在"),
	EXIST_APPLY("35","手机号被占用"),
	NO_EXIST_BANKCARD("36","银行卡不存在"),
	EXIST_BANKCARD("37","银行卡已存在"),
	MSG_SEND_ERROR("38","短信发送失败"),
	
	ERROR_DB_OPERATE("91","数据库操作失败"),
	
	DEAL_ING("99","处理中");
	
	private String code;
	private String msg;
	
	EnumRespStatusCode(String code, String msg) {
		this.code = code;
		this.msg = msg;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public boolean isSuccess() {
		if (this.getCode().equals("00")) {
			return true;
		} else {
			return false;
		}
	}
}
