package com.ucf.customer.dao.impl;

import org.springframework.stereotype.Repository;

import java.util.List;

import com.ucf.customer.pojo.UcfQuestion;
import com.ucf.customer.dao.UcfQuestionDao;
import com.ucf.customer.dao.base.BaseCustomerDao;
@Repository
public class UcfQuestionDaoImpl extends BaseCustomerDao implements UcfQuestionDao {

private static final long serialVersionUID = -1L;

	public void insertUcfQuestion(UcfQuestion ucfQuestion)throws Exception{
		this.insert("com.ucf.customer.insertUcfQuestion", ucfQuestion);
	}

	public void updateUcfQuestion(UcfQuestion ucfQuestion)throws Exception{
		this.update("com.ucf.customer.updateUcfQuestion", ucfQuestion);
	}

	public void deleteUcfQuestion(String quesNo)throws Exception{
		this.delete("com.ucf.customer.deleteUcfQuestion", quesNo);
	}

	public List<UcfQuestion> queryUcfQuestionListByWhere(UcfQuestion ucfQuestion)throws Exception{
		return this.queryForList("com.ucf.customer.queryUcfQuestionByWhere", ucfQuestion);
	}

}