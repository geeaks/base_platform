package com.ucf.customer.api.bo;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

/**
 * 用户银行卡列表
 * @author 张磊
 *
 */
public class BankCardBo implements java.io.Serializable{

	private static final long serialVersionUID = -1490187885576162447L;

	private String bankCardId;
	@SensitiveInfo(type=SensitiveType.BANK_CARD)
	private String cardNo;
	@SensitiveInfo(type=SensitiveType.CHINESE_NAME)
	private String cardName;
	private String cardType;
	private String bankCode;
	private String bankName;
	private String province;
	//认证方式
	private String certStatus;
	private String city;
	private String branchBankName;
	private String branchBankId;
	@SensitiveInfo(type=SensitiveType.ALL)
	private String cvn2;
	@SensitiveInfo(type=SensitiveType.ALL)
	private String validDate;
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String phone;
	private String status;
	/**
	 * 支付标识 : Y N
	 */
	private String isPay;
	/**
	 * 提现标识 : Y N
	 */
	private String isWithdraw;
	
	/**
	 * "F","理财卡","C","消费卡","S","结算卡"
	 */
	private String useType;
	
	public String getUseType() {
		return useType;
	}
	public void setUseType(String useType) {
		this.useType = useType;
	}
	
	public String getBankCardId() {
		return bankCardId;
	}
	public void setBankCardId(String bankCardId) {
		this.bankCardId = bankCardId;
	}
	
	public String getCertStatus() {
		return certStatus;
	}
	public void setCertStatus(String certStatus) {
		this.certStatus = certStatus;
	}
	public String getCardNo() {
		return cardNo;
	}
	public void setCardNo(String cardNo) {
		this.cardNo = cardNo;
	}
	public String getCardName() {
		return cardName;
	}
	public void setCardName(String cardName) {
		this.cardName = cardName;
	}
	public String getCardType() {
		return cardType;
	}
	public void setCardType(String cardType) {
		this.cardType = cardType;
	}
	public String getBankCode() {
		return bankCode;
	}
	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}
	public String getBankName() {
		return bankName;
	}
	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	public String getProvince() {
		return province;
	}
	public void setProvince(String province) {
		this.province = province;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getBranchBankName() {
		return branchBankName;
	}
	public void setBranchBankName(String branchBankName) {
		this.branchBankName = branchBankName;
	}
	public String getBranchBankId() {
		return branchBankId;
	}
	public void setBranchBankId(String branchBankId) {
		this.branchBankId = branchBankId;
	}
	public String getCvn2() {
		return cvn2;
	}
	public void setCvn2(String cvn2) {
		this.cvn2 = cvn2;
	}
	public String getValidDate() {
		return validDate;
	}
	public void setValidDate(String validDate) {
		this.validDate = validDate;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getIsPay() {
		return isPay;
	}
	public void setIsPay(String isPay) {
		this.isPay = isPay;
	}
	public String getIsWithdraw() {
		return isWithdraw;
	}
	public void setIsWithdraw(String isWithdraw) {
		this.isWithdraw = isWithdraw;
	}

}
