package com.ucf.customer.pojo;

import java.io.Serializable;
import java.util.Date;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

public class UcfUserLoginApp implements Serializable{

	 /**
	 * 
	 */
	private static final long serialVersionUID = 5003336193126205384L;

	private String userId;

	private String loginId;
	
	@SensitiveInfo(type=SensitiveType.ALL)
	private String loginPwd;
	
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String phone;

	@SensitiveInfo(type=SensitiveType.EMAIL)
	private String email;

	private String loginType;

	private Date gmtLastModified;

	private Date lastLoginTime;
	
	private String memo;

	private String frozenState;
	//会员类型
	private String userType;

		public void setUserId (String userId) {
			this.userId = userId;
		}

		public String getUserId () {
			return userId;
		}

		public void setLoginId (String loginId) {
			this.loginId = loginId;
		}

		public String getLoginId () {
			return loginId;
		}

		public void setLoginPwd (String loginPwd) {
			this.loginPwd = loginPwd;
		}

		public String getLoginPwd () {
			return loginPwd;
		}

		public void setPhone (String phone) {
			this.phone = phone;
		}

		public String getPhone () {
			return phone;
		}

		public void setEmail (String email) {
			this.email = email;
		}

		public String getEmail () {
			return email;
		}

		public void setLoginType (String loginType) {
			this.loginType = loginType;
		}

		public String getLoginType () {
			return loginType;
		}

		public void setGmtLastModified (Date gmtLastModified) {
			this.gmtLastModified = gmtLastModified;
		}

		public Date getGmtLastModified () {
			return gmtLastModified;
		}

		public void setLastLoginTime (Date lastLoginTime) {
			this.lastLoginTime = lastLoginTime;
		}

		public Date getLastLoginTime () {
			return lastLoginTime;
		}

		public void setMemo (String memo) {
			this.memo = memo;
		}

		public String getMemo () {
			return memo;
		}

		public void setFrozenState (String frozenState) {
			this.frozenState = frozenState;
		}

		public String getFrozenState () {
			return frozenState;
		}

		public String getUserType() {
			return userType;
		}

		public void setUserType(String userType) {
			this.userType = userType;
		}
		
}
