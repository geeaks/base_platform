public enum EnumCustomerCertStatus {
	T("T", "实名通过"),
	N("N", "实名未通过");
	private String code;
	private String desc;

	EnumCustomerCertStatus(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	
}
