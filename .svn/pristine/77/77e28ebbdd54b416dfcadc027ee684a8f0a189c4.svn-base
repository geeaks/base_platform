package com.ucf.customer.api.bo;

import com.ucf.customer.api.enums.EnumRespStatusCode;
import com.ucf.platform.framework.core.runtime.CommonResult;
/**
 * 
 * @author 
 *
 */
public class base_platformCommonResult<T> extends CommonResult<T> {

	private static final long serialVersionUID = -6076276477397685907L;

	public base_platformCommonResult(String resultCode, T businessObject) {
		super(resultCode, businessObject);
	}
	
	 public base_platformCommonResult(String resultCode, String resultCodeMsg, T businessObject) {
		  this(resultCode, businessObject);
	      this.resultCodeMsg = resultCodeMsg;
	 }

	@Override
	public boolean isSuccess() {
		 return this.getResultCode().equals(EnumRespStatusCode.SUCCESS.getCode());
	}
}
