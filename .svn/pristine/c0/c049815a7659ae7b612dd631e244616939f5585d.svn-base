	<![CDATA[
		INSERT INTO UCF_CARD (
	CARD_NO,CUSTOMER_ID,CARD_TYPE,PAYMENT_PASSWORD,ACCOUNT_ID,REPEAT_PAYPASSWORD_ERROR_TIMES,LAST_PAY_FAIL_DT,REGISTER_FROM,CERT_FROM,CERT_STATUS,FORBID_PAYMENT_TRANS,RUNTIME_STATUS,GMT_CREATE,GMT_MODIFIED,MEMO,LAST_PAY_DT,CERTIFY_INFO,USER_PROFILE,DISPLAY_ALIAS,CONTROL_FLAG,SUB_CARD_FLAG,OUTER_ALIAS,GESTURE_PASSWORD,SIMPLE_PASSWORD,CERT_ERROR_TIMES,USER_ID
		)VALUES(
		#{userId,jdbcType=VARCHAR},#{customerId,jdbcType=VARCHAR},#{cardType,jdbcType=CHAR},#{paymentPassword,jdbcType=VARCHAR},#{accountId,jdbcType=VARCHAR},#{repeatPaypasswordErrorTimes,jdbcType=NUMERIC},#{lastPayFailDt,jdbcType=TIMESTAMP},#{registerFrom,jdbcType=VARCHAR},#{certFrom,jdbcType=VARCHAR},#{certStatus,jdbcType=CHAR},#{forbidPaymentTrans,jdbcType=CHAR},#{runtimeStatus,jdbcType=CHAR},sysdate,sysdate,#{memo,jdbcType=VARCHAR},#{lastPayDt,jdbcType=TIMESTAMP},#{certifyInfo,jdbcType=VARCHAR},#{userProfile,jdbcType=VARCHAR},#{displayAlias,jdbcType=VARCHAR},#{controlFlag,jdbcType=VARCHAR},#{subCardFlag,jdbcType=CHAR},#{outerAlias,jdbcType=VARCHAR},#{gesturePassword,jdbcType=VARCHAR},#{simplePassword,jdbcType=VARCHAR},0,#{userId,jdbcType=VARCHAR})
	]]>
</insert>

