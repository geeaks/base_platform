window.onload = function showObj2(){
	var sUserAgent = navigator.userAgent;
	//根据操作系统类型，显示相应的插件
	if ("Mac"==detectOS()){
		document.getElementById("myTip").style.display="none";
		document.getElementById("myObj").style.display="none";
		document.getElementById("mac").style.display="block";
		
		document.getElementById("myTip1").style.display="none";
		document.getElementById("myObj1").style.display="none";
		document.getElementById("mac1").style.display="block";
		
		document.getElementById("myTip2").style.display="none";
		document.getElementById("myObj2").style.display="none";
		document.getElementById("mac2").style.display="block";
		//初始化mac插件，初始化时设置了延时，否则可能也现页面插件还没加载完初始化失败
		setTimeout(initMacPwd,1000);
	}else{
		var userpasswd = GetActiveX();
		//判断插件是否已经安装，如果已安装，则显示插件，未安装则显示下载链接提示
		if (userpasswd != null && userpasswd.Working==true){
			document.getElementById("myTip").style.display="none";
			document.getElementById("myObj").style.display="inline";
			document.getElementById("mac").style.display="none";
			
			document.getElementById("myTip1").style.display="none";
			document.getElementById("myObj1").style.display="inline";
			document.getElementById("mac1").style.display="none";
			
			document.getElementById("myTip2").style.display="none";
			document.getElementById("myObj2").style.display="inline";
			document.getElementById("mac2").style.display="none";
		}else{
			document.getElementById("myTip").style.display="inline-block";
			document.getElementById("myObj").style.display="none";
			document.getElementById("mac").style.display="none";
			
			document.getElementById("myTip1").style.display="inline-block";
			document.getElementById("myObj1").style.display="none";
			document.getElementById("mac1").style.display="none";
			
			document.getElementById("myTip2").style.display="inline-block";
			document.getElementById("myObj2").style.display="none";
			document.getElementById("mac2").style.display="none";
		}
	}
}
function confirmPwd2(){
