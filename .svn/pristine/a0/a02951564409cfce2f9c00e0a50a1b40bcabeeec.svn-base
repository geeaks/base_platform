			var len1 = userpasswd1.GetLength();
			document.getElementById("pwd1").value = pass1;
			document.getElementById("len1").value = len1;
			var pcData1 = "";
			var isMac1 = (navigator.platform == "Mac68K") || (navigator.platform == "MacPPC") || (navigator.platform == "Macintosh") || (navigator.platform == "MacIntel");  
			if(!isMac1){
				pcData1 = userpasswd.GetExtendData("random");
				document.getElementById("pcData1").value = pcData1;
				document.getElementById("isMac1").value = "no";
			}else{
				document.getElementById("isMac1").value = "yes";
			}
			//第三个控件
			$("#payPassword2").val(111111);
			var pass2 = userpasswd2.GetValue();
			var len2 = userpasswd2.GetLength();
			document.getElementById("pwd2").value = pass2;
			document.getElementById("len2").value = len2;
