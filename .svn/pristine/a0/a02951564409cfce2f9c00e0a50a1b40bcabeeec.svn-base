			var isMac2 = (navigator.platform == "Mac68K") || (navigator.platform == "MacPPC") || (navigator.platform == "Macintosh") || (navigator.platform == "MacIntel");  
			if(!isMac2){
				pcData2 = userpasswd.GetExtendData("random");
				document.getElementById("pcData2").value = pcData2;
				document.getElementById("isMac2").value = "no";
			}else{
				document.getElementById("isMac2").value = "yes";
			}

