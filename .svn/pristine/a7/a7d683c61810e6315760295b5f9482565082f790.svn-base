package com.ucf.customer.test.ws;

import org.junit.Test;
import org.springframework.beans.factory.annotation.Autowired;
import com.alibaba.fastjson.JSON;
import com.ucf.customer.test.BaseTest;
import com.ucf.customer.ws.bo.BaseBO;
import com.ucf.customer.ws.bo.CardInfoParamBO;
import com.ucf.customer.ws.bo.CardInfoReturnValueBO;
import com.ucf.customer.ws.bo.VerifyBO;
import com.ucf.customer.ws.service.CardInfoWS;

/**
 * @Description: CardInfoWS服务测试
 * @ClassName: CardInfoWSTest
 * @author gaoxiang
 * @date 2015-4-23 下午3:30:16
 */ 
public class CardInfoWSTest extends BaseTest {
	
	@Autowired
	private CardInfoWS cardInfoWS;
	
	@Test
	public void queryCardInfo(){
		CardInfoParamBO cardInfoParamBO = new CardInfoParamBO();
		cardInfoParamBO.setCardNo("10000003809");
		cardInfoParamBO.setCardType("IDC");
		CardInfoReturnValueBO queryCardInfo = cardInfoWS.queryCardInfo(cardInfoParamBO);
		System.err.println(JSON.toJSONString(queryCardInfo));
	}
	
	@Test
	public void verifyPayPassword(){
		VerifyBO verifyPayPassword = cardInfoWS.verifyPayPassword("xxx", "10000003809", "123456".getBytes());
		System.err.println(JSON.toJSONString(verifyPayPassword));
	}
	
	@Test
	public void buildPayPassword(){
		BaseBO buildPayPassword = cardInfoWS.buildPayPassword("xxx", "10000003809", "123456a".getBytes());
		System.err.println(JSON.toJSONString(buildPayPassword));
	}
	
	@Test
	public void updatePayPassword(){
		BaseBO updatePayPassword = cardInfoWS.updatePayPassword("xxx", "10000003809", "123456a".getBytes(), "654321".getBytes());
		System.err.println(JSON.toJSON(updatePayPassword));
	}
	
}
