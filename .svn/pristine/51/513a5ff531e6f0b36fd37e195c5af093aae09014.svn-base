package com.ucf.customer.ws.bo;

import java.io.Serializable;

import com.ucf.platform.framework.core.annotation.SensitiveInfo;
import com.ucf.platform.framework.core.util.SensitiveInfoUtils.SensitiveType;

public class WebServiceParamBO implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 4714867218381643000L;
	private String userId;
	@SensitiveInfo(type=SensitiveType.EMAIL)
	private String email;
	@SensitiveInfo(type=SensitiveType.MOBILE_PHONE)
	private String mobilePhone;
	private String certId;
	private String certType;
	private String merchantId;
	@SensitiveInfo(type=SensitiveType.BANK_CARD)
	private String cardNo;
	private String id;
	@SensitiveInfo(type=SensitiveType.CHINESE_NAME)
	private String realName;
	@SensitiveInfo(type=SensitiveType.ID_CARD)
	private String certNo;
	private String type;
	private String reqReserved;
	private String signature;
	
	private String loginflag;//联合登录两次接口的区别标志,0:初次调用，1:二次调用
	private String token;
	
	
	

	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getReqReserved() {
		return reqReserved;
	}
	public void setReqReserved(String reqReserved) {
		this.reqReserved = reqReserved;
	}
	public String getSignature() {
		return signature;
	}
	public void setSignature(String signature) {
		this.signature = signature;
	}
	public String getToken() {
		return token;
	}
	public void setToken(String token) {
		this.token = token;
	}
	public String getLoginflag() {
		return loginflag;
	}
	public void setLoginflag(String loginflag) {
		this.loginflag = loginflag;
	}
	public String getCardNo() {
		return cardNo;
	}
	public void setCardNo(String cardNo) {
		this.cardNo = cardNo;
	}
	public String getRealName() {
		return realName;
	}
	public void setRealName(String realName) {
		this.realName = realName;
	}
	public String getCertNo() {
		return certNo;
	}
	public void setCertNo(String certNo) {
		this.certNo = certNo;
	}
	public String getMerchantId() {
		return merchantId;
	}
	public void setMerchantId(String merchantId) {
		this.merchantId = merchantId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getMobilePhone() {
		return mobilePhone;
	}
	public void setMobilePhone(String mobilePhone) {
		this.mobilePhone = mobilePhone;
	}
	public String getCertId() {
		return certId;
	}
	public void setCertId(String certId) {
		this.certId = certId;
	}
	public String getCertType() {
		return certType;
	}
	public void setCertType(String certType) {
		this.certType = certType;
	}
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((cardNo == null) ? 0 : cardNo.hashCode());
		result = prime * result + ((certId == null) ? 0 : certId.hashCode());
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		WebServiceParamBO other = (WebServiceParamBO) obj;
		if (cardNo == null) {
			if (other.cardNo != null)
				return false;
		} else if (!cardNo.equals(other.cardNo))
			return false;
		if (certId == null) {
			if (other.certId != null)
				return false;
		} else if (!certId.equals(other.certId))
			return false;
		return true;
	}
	@Override
	public String toString() {
		return "WebServiceParamBO [userId=" + userId + ", email=" + email
				+ ", mobilePhone=" + mobilePhone + ", certId=" + certId
				+ ", certType=" + certType + ", merchantId=" + merchantId
				+ ", cardNo=" + cardNo + ", realName=" + realName + ", certNo="
				+ certNo + ", loginflag=" + loginflag + ", token=" + token
				+ "]";
	}

}
