<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" contentType="text/html; charset=utf-8"%>
<%
 response.setHeader("Pragma","No-cache");
 response.setHeader("Cache-Control","no-cache");
 response.setDateHeader("Expires",   0);
 %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
<title>先锋支付--用户注册</title>
<jsp:include page="/WEB-INF/views/jsp/common/oldcommon.jsp" />
<link href="${staticbase_platformPath}/css/bootstrap.css" rel="stylesheet">
<link rel="stylesheet" type="text/css" href="${staticbase_platformPath}/new/css/host_findpassword.css" />
<link href="${staticbase_platformPath}/new/css/person_login.css" rel="stylesheet" type="text/css" />
<link href="${staticbase_platformPath}/new/css/frist-register.css" rel="stylesheet" type="text/css" />
</head>
<body>
	<c:import url="headforregister.jsp"></c:import>
	<section class="bgc-gray">
	<div class="gs-center register">
		<img src="${staticbase_platformPath}/images/fund/asd_04.png">
		<div class="tab-content help-tab-content border-none">
			<div class="tab-pane active">
				<div class="background-wite">
					<div class="success-inf ">
						<div class="success-inf-inner">
							<span class="big-font">哇哦，成功啦！</span> 
							<span id="times" name="times" class="red">5</span>秒后为您跳转</br>
							</br>
							<p>
								恭喜你注册成功，并拥有先锋支付专属通行证！</br>
								请保管好你的通行徽章：${username}</br>
								页面若未跳转，请点此
								<a href="${basePath}/">手动跳转</a>
							</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<c:import url="/WEB-INF/views/jsp/common/newfooter.jsp"></c:import>
</body>
<script type="text/javascript">
var count=5;
function countTime(){
	count--;
	if(count>0){
		setTimeout("countTime();",1000);
		$("#times").html(count);
	}else{
		location.href="${basePath}/";
	}
}
setTimeout("countTime();",1000);
</script>
</html>