public enum EnumBindCardType {
	P("P", "预绑卡"), O("O", "正式绑卡"), U("U", "修改"), W("W", "提现银行卡");

	private String code;
	private String msgS;
	
	EnumBindCardType(String code, String msg) {
		this.code = code;
		this.msgS = msg;
	}
	
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getMsgS() {
		return msgS;
	}
	public void setMsgS(String msgS) {
		this.msgS = msgS;
	}
	
	
}
