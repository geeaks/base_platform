<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD  Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<!-- Entities 参数实体 -->

		<typeAlias type="java.lang.String" alias="string"/>	
		<typeAlias type="java.lang.Integer" alias="int"/>	
		<typeAlias type="com.ucf.onlinepay.framework.common.bean.Money" alias="MONEY"/>
		<typeAlias type="com.ucf.customer.pojo.UcfCustomer" alias="ucfCustomer" />
		<typeAlias type="com.ucf.customer.pojo.UcfPerson" alias="ucfPerson" />
		<typeAlias type="com.ucf.customer.pojo.UcfCard" alias="ucfCard" />
		<typeAlias type="com.ucf.customer.pojo.UcfBankCard" alias="ucfBankCard" />
		<typeAlias type="com.ucf.customer.pojo.UcfSubCard" alias="ucfSubCard" />
		<typeAlias type="com.ucf.customer.pojo.UcfUser" alias="ucfUser" />
		<typeAlias type="com.ucf.customer.pojo.UcfMerchantExtra" alias="ucfMerchantExtra" />
		<typeAlias type="com.ucf.customer.pojo.UcfQuestion" alias="ucfQuestion" />
		<typeAlias type="com.ucf.customer.pojo.UcfDistrict" alias="ucfDistrict"/>
		<typeAlias type="com.ucf.customer.pojo.UcfCity" alias="ucfCity"/>
		<typeAlias type="com.ucf.customer.pojo.UcfProvince" alias="ucfProvince"/>
		<typeAlias type="com.ucf.customer.pojo.UcfUserLoginApp" alias="ucfUserLoginApp"/>
		<typeAlias type="com.ucf.customer.pojo.UcfUserHabit" alias="ucfUserHabit"/>
		<typeAlias type="com.ucf.customer.pojo.UcfTradeLog" alias="ucfTradeLog"/>
		<typeAlias type="com.ucf.customer.pojo.UcfRealnamePhotoApply" alias="ucfRealnamePhotoApply"/>
		<typeAlias type="com.ucf.customer.ws.bo.UcfRealnamePhotoApplyQueryBO" alias="ucfRealnamePhotoApplyQueryBO"/>
		<typeAlias type="com.ucf.customer.ws.bo.UcfRealnamePhotoApplyBO" alias="ucfRealnamePhotoApplyBO"/>
	</typeAliases>
	<typeHandlers>
		<typeHandler javaType="com.ucf.onlinepay.framework.common.bean.Money" jdbcType="NUMERIC"
			 handler="com.ucf.customer.dao.base.MoneyTypeHandler"/>
	</typeHandlers>
	
	<plugins>
		<plugin interceptor="com.ucf.customer.dao.plugin.PagePlugin">
			<property name="dialect" value="oracle"/>
			<property name="pageSqlId" value=".*listPage.*"/>
		</plugin>
	</plugins>
<!-- 	
	<plugins>
		数据库分页方言
		<plugin
			interceptor="com.ucf.customer.dao.interceptor.DialectPaginationInterceptor">
			<property name="dialectClass"
				value="com.ucf.customer.dao.interceptor.MysqlDialect" />
		</plugin>
	</plugins> -->
</configuration>

