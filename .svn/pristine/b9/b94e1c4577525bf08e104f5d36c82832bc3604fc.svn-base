package com.ucf.customer.api.bo;
import com.ucf.onlinepay.framework.common.bean.Money;
public class AccountInfoRsBO extends BaseResultBO {

	private static final long serialVersionUID = -4126380587725945639L;

	/**
	 * 会员卡No
	 */
	private String cardNo;
	/**
	 * 资金账户ID
	 */
	private String accountId;
	/**
	 * 余额
	 */
	private Money balance;
	/**
	 * 信用余额
	 */
	private Money creditAmount;
	
	/**
	 * 冻结余额
	 */
	private Money freezeAmount;

	public String getCardNo() {
		return cardNo;
	}

	public void setCardNo(String cardNo) {
		this.cardNo = cardNo;
	}

	public String getAccountId() {
		return accountId;
	}

	public void setAccountId(String accountId) {
		this.accountId = accountId;
	}

	public Money getBalance() {
		return balance;
	}

	public void setBalance(Money balance) {
		this.balance = balance;
	}

	public Money getCreditAmount() {
		return creditAmount;
	}

	public void setCreditAmount(Money creditAmount) {
		this.creditAmount = creditAmount;
	}
	
	public Money getFreezeAmount() {
		return freezeAmount;
	}

	public void setFreezeAmount(Money freezeAmount) {
		this.freezeAmount = freezeAmount;
	}

	@Override
	public String toString() {
		return "AccountInfoRsBO [cardNo=" + cardNo + ", accountId=" + accountId
				+ ", balance=" + balance + ", freezeAmount=" + freezeAmount + 
				", creditAmount=" + creditAmount
				+ ", toString()=" + super.toString() + "]";
	}

	

}
