public enum EnumTrueFalse {
	TRUE("TRUE", "成功"), FALSE("FALSE", "失败");
	private String code;
	private String desc;

	EnumTrueFalse(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	

}
