<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<c:set var="basePath" value="${pageContext.request.contextPath }" />




<input type="hidden" id="baseUrl" value="${basePath}" />

											<div style="display:inline;">
											<div id="myTip" style="width:220px;height:28px;border:1px solid #888888;background:#FFFFFF;display:none;" align="center" valign="middle">
												<a id='downLink' href="${staticbase_platformPath}/${myexe}" class="mylink">
													<font face="宋体" size="-1">点此下载控件</font>
												</a>
											</div>
											<div id="myObj" class='' style="display:inline;float:left;"  align="left">
												<!-- windows版本密码控件  -->
												<% if(request.getHeader("User-Agent").contains("Mac OS")==false){ 
													//如果是mac系统页面不加载windows版本插件 %>
												<object id="userpasswd" style="width:220px;height:40px;margin-left:2px; margin-top:-8px;"  classid="CLSID:598BD8F9-CE1C-4b52-9C0E-6EA55E2371A7" tabIndex="2"
													
													
													codebase="${staticbase_platformPath}/${mycab}#Version=${myVersion}" data-tip="6-20位字符，必须包含：大写字母、小写字母、数字、符号至少两种">
														
													<param name="textsize" value="16"> 
													<param name="color" value="#000000"> 
													<param name="bgcolor" value="#FFFFFF">  
													<param name="bordercolor" value="#ccc">  
													<param name="MyTime" value="${serverTime}">
												<embed id="userpasswdNP" style="width:220px;height:40px;margin-left:2px; margin-top:-8px;" type="application/598BD8F9-CE1C-4b52-9C0E-6EA55E2371A7" color="#000000" bgcolor="#FFFFFF" bordercolor="#cccccc" width="220px" height="28px" textsize="16px" MyTime="${serverTime}" tabIndex="2"  data-tip="6-20位字符，必须包含：大写字母、小写字母、数字、符号至少两种"></embed>
												</object>
												<%} %>
												<!-- <span class="msg-box n-right" for="pwd" style="">
												<span class="msg-wrap n-error" role="alert">
												<span class="n-icon"></span>
												<span class="n-msg">不能为空</span>
												</span>
												</span> -->
											</div>
											<div id="mac" style="display:none" align="left">
												<!-- MAC版本密码控件  -->
												<% if(request.getHeader("User-Agent").contains("Mac OS")){ 
													//如果是mac系统,页面才加载mac版本插件 %>
												<embed id="macPasswd" width="220" height="30" type="application/598BD8F9-CE1C-4b52-9C0E-6EA55E2371A7" ></embed>	
										
												<% } %>
											</div>
