import java.io.Serializable;
import java.util.Map;

public class AccBalanceReturnValueBO implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = -4822914341878155612L;
	/**
	 * KEY 1.result 执行结果 1.成功 0.失败 
	 * 2.message 执行结果信息 
	 * 3.exceptionCode 异常Code
	 * 4.exceptionMessage 异常提示信息 
	 *
	 */
	private Map<Object, Object> returnValueMap;
	private String balance;

	public String getBalance() {
		return balance;
	}

	public void setBalance(String balance) {
		this.balance = balance;
	}

	public Map<Object, Object> getReturnValueMap() {
		return returnValueMap;
	}

	public void setReturnValueMap(Map<Object, Object> returnValueMap) {
		this.returnValueMap = returnValueMap;
	}

}
